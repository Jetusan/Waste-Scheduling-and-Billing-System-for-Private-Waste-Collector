{
  "expo": {
    "name": "WSBS - Waste Management",
    "slug": "wsbs-waste-management",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/images/LOGO.png",
    "scheme": "wsbs",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Waste Scheduling and Billing System for Private Waste Collectors",
    "privacy": "public",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wsbs.wastemanagement"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LOGO.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.WSBS",
      "versionCode": 6,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "wsbs"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ac7dcefc-ef5b-4704-b66e-1f9094f0e759"
      }
    }
  }
}
